{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-aqi-md-dev"
		},
		"ls_adls_aqi_medallion_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_aqi_medallion'"
		},
		"syn-aqi-md-dev-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'syn-aqi-md-dev-WorkspaceDefaultSqlServer'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=tcp:syn-aqi-md-dev.sql.azuresynapse.net,1433;Initial Catalog=@{linkedService().DBName}"
		},
		"ls_adls_aqi_medallion_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlaqimddev.dfs.core.windows.net/"
		},
		"ls_keyvault_aqi_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvaqidataprojectdev.vault.azure.net/"
		},
		"ls_rest_datagovin_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.data.gov.in"
		},
		"syn-aqi-md-dev-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlaqimddev.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-aqi-md-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_aqi_medallion_accountKey": {
						"value": "[parameters('ls_adls_aqi_medallion_accountKey')]"
					},
					"syn-aqi-md-dev-WorkspaceDefaultSqlServer_connectionString": {
						"value": "[parameters('syn-aqi-md-dev-WorkspaceDefaultSqlServer_connectionString')]"
					},
					"ls_adls_aqi_medallion_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_aqi_medallion_properties_typeProperties_url')]"
					},
					"ls_keyvault_aqi_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_aqi_properties_typeProperties_baseUrl')]"
					},
					"ls_rest_datagovin_properties_typeProperties_url": {
						"value": "[parameters('ls_rest_datagovin_properties_typeProperties_url')]"
					},
					"syn-aqi-md-dev-WorkspaceDefaultStorage_properties_typeProperties_url": {
						"value": "[parameters('syn-aqi-md-dev-WorkspaceDefaultStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}